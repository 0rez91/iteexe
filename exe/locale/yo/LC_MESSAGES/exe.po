msgstr ""

#: exe/jsui/scripts/app/view/forms/MetadataPanel.js:728
msgid "The topic of the content of the resource."
msgstr ""

#: exe/jsui/scripts/app/view/forms/MetadataPanel.js:731
msgid ""
