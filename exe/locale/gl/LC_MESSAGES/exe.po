#: exe/engine/orientacionesalumnadofpdidevice.py:31
msgid ""
"Orientciones alumnado es un iDevice que permite al alumnado conocer los "
"objetivos de su aprendizaje."
msgstr ""

#: exe/engine/orientacionesalumnadofpdidevice.py:37
#, fuzzy
msgid "Texto Orientaciones alumnado"
msgstr "FPD - Orientacións Alumnado"

#: exe/engine/orientacionesalumnadofpdidevice.py:135
